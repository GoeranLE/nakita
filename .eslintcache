itas.slice(0, 300).filter(kita=> {\nif(userInput) return kita.plz === userInput;\nreturn kita;\n}).map(kita => {\n                    return (\n\n                        <Marker \n                        key={kita.id}\n                        latitude={kita.alat}\n                        longitude={kita.alon}\n                    >\n                        <button \n                        className=\"marker-btn\"\n                        onClick={e => {\n                        e.preventDefault();\n                        setSelectedKita(kita);\n                            }}\n                        >\n                        \n                        <img src={icon} alt=\"Baby Icon\" className=\"baby\" />\n                        </button>\n                    </Marker>\n\n                    )\n                }\n                )}\n                {selectedKita ? (\n          <Popup\n            latitude={selectedKita.alat}\n            longitude={selectedKita.alon}\n            onClose={() => {\n              setSelectedKita(null);\n            }}\n          >\n            <div>\n              <h2>{selectedKita.name}</h2>\n              <p>{selectedKita.address}</p>\n              <p>{selectedKita.plz}</p>\n              <p>{selectedKita.district}</p>\n            </div>\n          </Popup>\n        ) : null}\n            </ReactMapGL>\n          </Col>\n        </Row>\n      </Container>\n    </div>\n  );\n}\n",["61","62"],"/Users/goranlehmann/Desktop/WBS/WBSProjects/FINAL/NaKita/src/assets/components/Footer.js",["63","64"],"/Users/goranlehmann/Desktop/WBS/WBSProjects/FINAL/NaKita/src/assets/components/Searchbar.js",[],["65","66"],"/Users/goranlehmann/Desktop/WBS/WBSProjects/FINAL/NaKita/src/index.js",[],["67","68"],"/Users/goranlehmann/Desktop/WBS/WBSProjects/FINAL/NaKita/src/assets/components/NotFound.js",[],["69","70"],"/Users/goranlehmann/Desktop/WBS/WBSProjects/FINAL/NaKita/src/assets/components/Intro.js",[],"/Users/goranlehmann/Desktop/WBS/WBSProjects/F